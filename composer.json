{
    "name": "dpc-sdp/vicgovau",
    "description": "Drupal 8 site for Vicgovau",
    "license": "proprietary",
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "^1.6.0",
        "dpc-sdp/bay": "^0.0.1",
        "dpc-sdp/tide": "1.1.6",
        "dpc-sdp/tide_alert": "^0.0.1",
        "dpc-sdp/tide_data_driven_component": "^0.0.1",
        "dpc-sdp/tide_grant": "^0.0.1",
        "dpc-sdp/tide_profile": "^0.0.1",
        "dpc-sdp/tide_publication": "^0.0.1",
        "dpc-sdp/tide_authenticated_content": "^0.0.1",
        "drupal-composer/drupal-scaffold": "^2.3",
        "drupal/admin_toolbar": "^1.20",
        "drupal/clamav": "^1.1",
        "drupal/coffee": "^1.0@beta",
        "drupal/config_filter": "^1.2",
        "drupal/default_content": "^1.0@alpha",
        "drupal/devel": "^1.2",
        "drupal/field_tools": "^1.0-alpha4",
        "drupal/lagoon_logs": "^1.0",
        "drupal/openapi": "^1.0-beta2",
        "drupal/openapi_ui": "^1.0-rc1",
        "drupal/openapi_ui_redoc": "^1.0-rc2",
        "drupal/permissions_by_term": "^2.6",
        "drupal/profile": "^1.0@RC",
        "drupal/migrate_cron": "^1.1",
        "drupal/migrate_tools": "^4.1",
        "drupal/range": "^1.1",
        "drupal/shield": "^1.2",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/username_enumeration_prevention": "^1.0@beta",
        "drush/drush": "^8.1",
        "library/dropzone": "^4.3",
        "webflo/drupal-finder": "^1.1",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "behat/mink-selenium2-driver": "1.3.x-dev",
        "drupal/config_devel": "^1.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "drupal/coder": "^8.2.12",
        "drupal/console": "^1.0",
        "drupal/drupal-extension": "^3.4",
        "guzzlehttp/psr7": "^1.4",
        "integratedexperts/behat-format-progress-fail": "^0.2.1",
        "integratedexperts/behat-screenshot": "^0.7",
        "integratedexperts/behat-steps": "^0.1.0",
        "phpcompatibility/php-compatibility": "^9.1",
        "php-http/curl-client": "^1.7",
        "symfony/filesystem": "^3.2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "docroot/core",
            "dpc-sdp",
            "tests/unit/"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": {
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "dpc-sdp/bay": {
            "type": "path",
            "url": "dpc-sdp/bay"
        },
        "dpc-sdp/tide_alert": {
            "type": "path",
            "url": "dpc-sdp/tide_alert"
        },
        "dpc-sdp/tide_grant": {
            "type": "path",
            "url": "dpc-sdp/tide_grant"
        },
        "dpc-sdp/tide_publication": {
            "type": "path",
            "url": "dpc-sdp/tide_publication"
        },
        "dpc-sdp/tide": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide.git"
        },
        "dpc-sdp/tide_api": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_api.git"
        },
        "dpc-sdp/tide_core": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_core.git"
        },
        "dpc-sdp/tide_data_driven_component": {
          "type": "path",
          "url": "dpc-sdp/tide_data_driven_component"
        },
        "dpc-sdp/tide_event": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_event.git"
        },
        "dpc-sdp/tide_authenticated_content": {
            "type": "path",
            "url": "dpc-sdp/tide_authenticated_content"
        },
        "dpc-sdp/tide_landing_page": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_landing_page.git"
        },
        "dpc-sdp/tide_media": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_media.git"
        },
        "dpc-sdp/tide_monsido": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_monsido.git"
        },
        "dpc-sdp/tide_page": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_page.git"
        },
        "dpc-sdp/tide_profile": {
            "type": "path",
            "url": "dpc-sdp/tide_profile"
        },
        "dpc-sdp/tide_news": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_news.git"
        },
        "dpc-sdp/tide_search": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_search.git"
        },
        "dpc-sdp/tide_site": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_site.git"
        },
        "dpc-sdp/tide_test": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_test.git"
        },
        "dpc-sdp/tide_webform": {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/dpc-sdp/tide_webform.git"
        },
        "library/dropzone": {
            "type": "package",
            "package": {
                "name": "library/dropzone",
                "version": "4.3.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
                    "type": "zip"
                }
            }
        },
        "package/ckeditor-iframe": {
            "type": "package",
            "package": {
                "name": "ckeditor/iframe",
                "type": "drupal-library",
                "version": "4.5.11",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/iframe/releases/iframe_4.5.11.zip",
                    "reference": "master"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "package/ckeditor-fakeobjects": {
            "type": "package",
            "package": {
                "name": "ckeditor/fakeobjects",
                "type": "drupal-library",
                "version": "4.5.11",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.5.11.zip",
                    "reference": "master"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "package/ckeditor-templates": {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "type": "drupal-library",
                "version": "4.5.7",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.5.7.zip",
                    "reference": "master"
                }
            }
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "source": "http://cgit.drupalcode.org/drupal/plain/{path}?h={version}",
            "includes": [
                ".editorconfig",
                ".eslintignore",
                ".gitattributes",
                ".htaccess",
                "index.php",
                "update.php"
            ],
            "excludes": [
                "robots.txt"
            ],
            "omit-defaults": true
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "preserve-paths": [
            "docroot/modules/custom",
            "docroot/themes/custom",
            "docroot/drush",
            "docroot/sites/default/settings.php",
            "docroot/sites/default/default.settings.local.php",
            "docroot/sites/default/files"
        ],
        "patches": {
            "behat/gherkin": {
                "Correctly filter features, now that the base path is correctly set - https://github.com/Behat/Gherkin/pull/129": "https://patch-diff.githubusercontent.com/raw/Behat/Gherkin/pull/129.patch"
            },
            "cweagans/composer-patches": {
                "Fix for already applied patches - specific to 1.6.5": "patches/composer-patches/fix-applied-patches.patch"
            },
            "drupal/core": {
                "Debug EntityChangedConstraintValidator": "./patches/core/drupal-core-debug-entity-changed-constraint-validator.patch",
                "Temporary files whose files are missing on the disk result in never-ending error log messages - https://www.drupal.org/project/drupal/issues/2802803": "https://www.drupal.org/files/issues/2018-06-27/missing-temp-files-stuck-in-table-2802803-33.patch"
            },
            "drupal/default_content": {
                "Do not reimport existing entities - https://www.drupal.org/project/default_content/issues/2698425": "https://www.drupal.org/files/issues/2018-06-05/default_content-dont-reimport-existing-entities-2698425-102.patch"
            },
            "drupal/purge_queuer_url": {
                "Fixing the SQL Sanitize Drush command - https://www.drupal.org/project/purge_queuer_url/issues/2950814": "https://www.drupal.org/files/issues/2950814-drush-sanitize-service-check.patch"
            },
            "drupal/password_policy": {
                "Allow users to bypass validation when editing another user, and correct permission is set - https://www.drupal.org/project/password_policy/issues/2786315": "https://www.drupal.org/files/issues/2019-02-15/edit_other_users-2786315-38.patch",
                "Add password validation to the user entity as a constraint - https://www.drupal.org/project/password_policy/issues/2941526#comment-13004347": "https://www.drupal.org/files/issues/2019-03-12/password_policy-move-password-validation-to-user-entity-2941526-6-D8.patch"
            },
            "dpc-sdp/tide_media": {
                "tide_media field type change patch - https://github.com/dpc-sdp/tide_media/pull/23": "https://patch-diff.githubusercontent.com/raw/dpc-sdp/tide_media/pull/23.patch"
            }
        }
    }
}
